{
  "name": "wizard-console",
  "version": "0.1.0",
  "license": "MIT",
  "description": "WizardConsole - MQTT Race Hackathon Platform",
  "scripts": {
    "postinstall": "echo 'Installation termin√©e'",
    "dev:backend": "nodemon packages/backend/src/main.ts",
    "dev:frontend": "cd packages/frontend && ng serve",
    "build:backend": "tsc -p packages/backend/tsconfig.json",
    "build:frontend": "cd packages/frontend && ng build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "mqtt:test": "mosquitto_sub -h localhost -p 1883 -t '#'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/websockets": "^10.0.2",
    "@nestjs/platform-socket.io": "^10.0.2",
    "mqtt": "^4.3.7",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.1.13",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
