    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/mqtt": "^2.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  }
